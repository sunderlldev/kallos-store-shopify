/* Head */
@font-face {
  font-family: 'Cormorant CS';
  src: url('{{ "CormorantCS-Bold.ttf" | asset_url }}') format('truetype');
  font-weight: 700;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Cormorant CS';
  src: url('{{ "CormorantCS-SemiBold.ttf" | asset_url }}') format('truetype');
  font-weight: 600;
  font-style: normal;
  font-display: swap;
}

/* Body */
@font-face {
  font-family: 'Adelle Sans';
  src: url('{{ "Adelle_Sans_ARA_Regular.otf" | asset_url }}') format('opentype');
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}