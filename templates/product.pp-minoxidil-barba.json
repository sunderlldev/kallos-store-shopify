        "custom_liquid_eWUnTT": {
          "type": "custom-liquid",
          "name": "Follics Vs Otros",
          "settings": {
            "custom_liquid": "<div class=\"kallos-comparison-full\">\n  <div class=\"kallos-table-wrapper\">\n    <table class=\"kallos-table\">\n      <thead>\n        <tr>\n          <th class=\"empty-cell\"></th>\n          <th class=\"col-featured\">\n            <div class=\"badge-wrapper\">\n              <div class=\"img-container main-img\">\n                <img src=\"https://cdn.shopify.com/s/files/1/0790/9497/2665/files/MOCKUP_FOLLICS_1_PARA_COMPARATIVA.png?v=1768926053\" alt=\"Kallos\">\n              </div>\n              <h5 class=\"table-header-title\">FOLLICS By Kallos</h5>\n            </div>\n          </th>\n          <th class=\"col-others\">\n            <div class=\"badge-wrapper\">\n              <div class=\"img-container\">\n                <img src=\"https://cdn.shopify.com/s/files/1/0790/9497/2665/files/MOCKUP_FOLLICS_PARA_COMPARATIVA.png?v=1768926052\" alt=\"Others\" class=\"img-others\">\n              </div>\n              <h5 class=\"table-header-title\">OTROS</h5>\n            </div>\n          </th>\n        </tr>\n      </thead>\n      <tbody>\n        {% assign features = \"Minoxidil 5%,Base en crema ligera,Rápida absorción,No deja sensación grasosa,Incluye activos naturales,Irritación y resequedad\" | split: \",\" %}\n        {% for feature in features %}\n        <tr class=\"comparison-row\">\n          <td class=\"feature-label\"><h5>{{ feature }}</h5></td>\n\n          <td class=\"cell-featured\">\n            <div class=\"icon-container\">\n              <img src=\"https://forchics.com/cdn/shop/files/checked_1.webp?v=1707502828\" class=\"status-icon\">\n            </div>\n          </td>\n\n          <td class=\"cell-others\">\n            <div class=\"icon-container\">\n              {% if forloop.first or forloop.last %}\n                <img src=\"https://forchics.com/cdn/shop/files/checked_1.webp?v=1707502828\" class=\"status-icon\">\n              {% else %}\n                <img src=\"https://forchics.com/cdn/shop/files/cancel_1.webp?v=1707502858\" class=\"status-icon\">\n              {% endif %}\n            </div>\n          </td>\n        </tr>\n        {% endfor %}\n      </tbody>\n    </table>\n  </div>\n</div>\n\n<style>\n.kallos-comparison-full {\n  width: 100%;\n  max-width: 900px;\n  margin: 0 auto;\n  --main-accent: #AC9651;\n  --row-bg: #c0b67c;\n}\n\n.kallos-table-wrapper {\n  width: 100%;\n  padding-top: 110px;\n}\n\n.kallos-table {\n  width: 100%;\n  border-collapse: collapse;\n  table-layout: fixed;\n}\n\n.badge-wrapper {\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-end;\n  min-height: 60px;\n}\n\n.img-container {\n  position: absolute;\n  bottom: 45px;\n  display: flex;\n  justify-content: center;\n  width: 100%;\n  z-index: 10;\n}\n\n.main-img img { width: 86px; height: 72px; }\n.img-others { width: 46px; height: 72px; }\n\n.kallos-table h5 {\n  margin: 0;\n  font-weight: 700;\n  line-height: 1.2;\n  font-size: 14px;\n}\n\n.table-header-title {\n  padding: 12px 0;\n  font-size: 1.05em;\n  font-weight: 800;\n  text-align: center;\n  width: 100%;\n  position: relative;\n  z-index: 5;\n}\n\n.col-featured {\n  background: var(--main-accent);\n  border-radius: 20px 20px 0 0;\n  color: white;\n}\n\n.col-others {\n  color: black;\n}\n\n.empty-cell { width: 32%; }\n\n.kallos-table td {\n  padding: 15px 10px;\n  border: none;\n}\n\n.comparison-row:first-child .feature-label { border-radius: 15px 0 0 0; }\n.comparison-row:last-child .feature-label { border-radius: 0 0 0 15px; }\n.comparison-row:first-child .cell-others { border-radius: 0 15px 0 0; }\n.comparison-row:last-child .cell-others { border-radius: 0 0 15px 0; }\n\n.feature-label {\n  background: var(--row-bg);\n  color: white;\n  text-align: left;\n  padding-left: 22px;\n  width: 52%;\n}\n\n.cell-featured {\n  background: var(--main-accent);\n  width: 24%;\n}\n\n.cell-others {\n  background: var(--row-bg);\n  width: 24%;\n}\n\n.icon-container {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.status-icon {\n  width: 16px;\n  height: 16px;\n  filter: brightness(0) invert(1);\n}\n\n@media screen and (min-width: 769px) {\n  .kallos-comparison-full {\n    width: 75%;\n  }\n}\n\n@media screen and (max-width: 768px) {\n  .kallos-comparison-full {\n    width: 100%;\n    padding: 0 10px;\n  }\n  .kallos-table-wrapper { padding-top: 80px; }\n  .main-img img { width: 76px; height: 62px; }\n  .img-others { width: 42px; height: 68px; }\n  .table-header-title { font-size: 13px; }\n  .kallos-table h5 { \n  font-size: 11px;\n}\n  .table-header-title {   \n    width: 80%;\n}\n}\n</style>"
